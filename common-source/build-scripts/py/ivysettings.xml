<?xml version="1.0" encoding="UTF-8"?>
<ivy-settings>
  <property name="artifactory.root.url" value="https://artifacts.microchip.com/artifactory"/>
  <settings defaultResolver="ivy"/>
  <resolvers>
    <chain name="main">
      <url name="ivy" m2compatible="true">
        <artifact pattern="${artifactory.root.url}/ivy/[organisation]/[module]/[revision]/[artifact]-[revision](-[classifier]).[ext]"/>
        <ivy pattern="${artifactory.root.url}/ivy/[organisation]/[module]/[revision]/ivy-[revision](-[classifier]).xml"/>
      </url>
    </chain>
  </resolvers>
</ivy-settings>
